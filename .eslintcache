> \n                  <Card key={idx} id={loc.id} >\n                    <Image src='https://media.giphy.com/media/37QOWrwGOljUCbdDxd/giphy.gif' />\n                    <Card.Content>\n                      <Card.Header>{loc.name}</Card.Header>\n                      { star.repeat(parseInt(loc.reviews.map(rev => rev.rating).reduce((a,b) => a + b, 0) / loc.reviews.length)) } \n                      <Card.Meta>\n                        {loc.description}\n                      </Card.Meta>\n                    </Card.Content>\n                    <Card.Content extra>\n                      <Button id={loc.id}>\n                        <Icon name='star outline' />\n                        Favorite\n                      </Button>\n                      <Button id={loc.id} name={loc.name} as={NavLink} to='/location' onClick={e => this.renderLocation(e.target.id, e.target.name)} >\n                        View\n                      </Button>\n                    </Card.Content>\n                  </Card>\n                )}\n              </Card.Group>\n            :\n              <Card.Group centered>\n                {filteredArray.map((loc, idx) => \n                  <Card key={idx} id={loc.id} >\n                    <Image src='https://media.giphy.com/media/37QOWrwGOljUCbdDxd/giphy.gif' />\n                    <Card.Content>\n                      <Card.Header>{loc.name}</Card.Header>\n                      <Card.Meta>\n                        {loc.description}\n                      </Card.Meta>\n                    </Card.Content>\n                    <Card.Content extra>\n                      <Button id={loc.id}>\n                        <Icon name='star outline' />\n                        Favorite\n                      </Button>\n                      <Button id={loc.id} name={loc.name} as={NavLink} to='/location' onClick={e => this.renderLocation(e.target.id, e.target.name)} >\n                        View\n                      </Button>\n                    </Card.Content>\n                  </Card>\n                )}\n              </Card.Group>\n            }\n        </div>\n      </div>\n    );\n  }\n}\n\nfunction mdp(dispatch){\n  return {fetchLocations: () => dispatch(getLocationsFromApi()), setLocation: (id) => dispatch(setCurrentLocation(id))}\n}\n\nfunction msp(state){\n  return {locations: state.locations}\n}\n\nexport default connect(msp, mdp)(LocationList);","/Users/sagbeyeg/Flatiron/code/mod5/traverse/traverse-frontend/src/Components/Navbar.js",["103"],"/Users/sagbeyeg/Flatiron/code/mod5/traverse/traverse-frontend/src/index.js",[],["104","105"],"/Users/sagbeyeg/Flatiron/code/mod5/traverse/traverse-frontend/src/Redux/actions.js",[],"/Users/sagbeyeg/Flatiron/code/mod5/traverse/traverse-frontend/src/Redux/rootReducer.js",["106"],"import { combineReducers } from 'redux'\n\nconst defaultState = {\n  currentUser: [],\n  loggedIn: true,\n  user: [],\n  locations: [],\n  currentLocation: []\n  \n}\n\nfunction currentUserReducer(state = defaultState.currentUser, action) {\n  switch (action.type) {\n    case \"LOGIN\":\n      console.log(\"inside of current user reducer\")\n      console.log(\"payload:\", action.payload) \n      return action.payload\n    default:\n      return state\n  }\n}\n\nfunction userReducer(state = defaultState.user, action) {\n    switch (action.type) {\n      case \"FETCH_USER\":\n        console.log(\"inside of User reducer\", action)\n        return action.payload\n      default:\n        return state\n    }\n}\n\nfunction locationsReducer(state = defaultState.locations, action) {\n  switch (action.type) {\n    case \"FETCH_LOCATIONS\":\n      console.log(\"inside of locations reducer\", action)\n      return action.payload\n    default:\n      return state\n  }\n}\n\nfunction currentLocationReducer(state = defaultState.currentLocation, {type, payload}) {\n  switch (type) {\n      case \"VIEW_LOCATION\":\n        console.log(\"inside of current location reducer\", payload)\n          return payload\n      case \"ADD_REVIEW\"\n      :\n        console.log(\"inside of current location reducer\", payload)\n        console.log(\"State\", state)\n        let newObj = [...state.reviews, {...payload.review}]\n        return {...state, reviews: newObj }  \n      case \"UPDATE_REVIEW\":\n        console.log(\"inside of current location reducer\", payload)\n        let copiedArray = [...state.reviews]\n        let idx = copiedArray.findIndex(rev => rev.id == payload.id)\n        copiedArray[idx] = payload \n        return {...state, reviews: copiedArray } \n      case \"DELETE_REVIEW\":\n        console.log(\"inside of current location reducer\", payload)\n        let filteredReviews = state.reviews.filter(rev => rev.id !== payload)\n        console.log(\"Filtered Array:\", filteredReviews)\n        return {...state, reviews: filteredReviews }\n        case \"ADD_TRIP\":\n          console.log(\"inside of current location reducer\", payload)\n          let newTrip = [...state.trips, {...payload.trip}]\n          return {...state, trips: newTrip }  \n      default:\n          return state\n  }\n}\n\n\n\n\n// {type: \"INCREMENT_COUNTER\"}\nconst rootReducer = combineReducers({\n  currentUser: currentUserReducer,\n  user: userReducer,\n  locations: locationsReducer,\n  currentLocation: currentLocationReducer\n})\n\nexport default rootReducer","/Users/sagbeyeg/Flatiron/code/mod5/traverse/traverse-frontend/src/Components/Location.js",[],"/Users/sagbeyeg/Flatiron/code/mod5/traverse/traverse-frontend/src/Components/Review.js",["107"],"/Users/sagbeyeg/Flatiron/code/mod5/traverse/traverse-frontend/src/Components/Trip.js",["108","109"],"import React, { Component } from 'react';\nimport {Card, Icon} from 'semantic-ui-react'\n\nclass Trip extends Component {\n  render() {\n    const { trip } = this.props\n    return (\n      <div>\n      {trip?\n        <Card >\n          <Card.Content>\n          <Card.Header><em>{trip.location.name}</em></Card.Header>\n          <